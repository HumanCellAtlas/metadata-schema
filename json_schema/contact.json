{
    "description": "contact schema generate by tagSchemaToJson from contact", 
    "title": "contact", 
    "required": [
        "name", 
        "email"
    ], 
    "additionalProperties": false, 
    "definitions": {}, 
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "type": "object", 
    "properties": {
        "country_division": {
            "enum": [
                "AL", 
                "AK", 
                "AR", 
                "AZ", 
                "CA", 
                "CO", 
                "CT", 
                "DE", 
                "FL", 
                "GA", 
                "HI", 
                "ID", 
                "IA", 
                "IL", 
                "IN", 
                "KS", 
                "KY", 
                "LA", 
                "MA", 
                "MD", 
                "ME", 
                "MI", 
                "MN", 
                "MO", 
                "MS", 
                "MT", 
                "NC", 
                "ND", 
                "NE", 
                "NH", 
                "NJ", 
                "NM", 
                "NV", 
                "NY", 
                "OH", 
                "OK", 
                "OR", 
                "PA", 
                "RI", 
                "SC", 
                "SD", 
                "TN", 
                "TX", 
                "UT", 
                "VT", 
                "VA", 
                "WA", 
                "WV", 
                "WI", 
                "XY", 
                "FU", 
                "PR", 
                "VI"
            ], 
            "description": "Name of state, province, canton, or other country subdivision."
        }, 
        "city": {
            "type": "string", 
            "description": "Name of city where contact works."
        }, 
        "name": {
            "type": "string", 
            "description": "The contact's name. Should be in the format first, middle, last name. Middle can be initial or left blank. e.g. John,D,Doe or Jane,,Smith"
        }, 
        "country": {
            "enum": [
                "Austria", 
                "Canada", 
                "China", 
                "France", 
                "Germany", 
                "Israel", 
                "Japan", 
                "Sweden", 
                "Switzerland", 
                "UK", 
                "USA"
            ], 
            "description": "Name of country where contact works. Please spell out (except for USA or UK)."
        }, 
        "institution": {
            "type": "string", 
            "description": "Name of primary institute where contact works."
        }, 
        "phone": {
            "type": "string", 
            "description": "Phone number (including country code) of contact or contact's lab."
        }, 
        "address": {
            "type": "string", 
            "description": "Full mailing address where contact works. Should include street name and number, city, state, postal code, country."
        }, 
        "laboratory": {
            "type": "string", 
            "description": "Name of lab (often the PI name) within institute where contact works."
        }, 
        "email": {
            "pattern": "^.*@.*..*$", 
            "type": "string", 
            "description": "An email address for the contact."
        }
    }
}