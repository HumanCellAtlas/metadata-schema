{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://schema.humancellatlas.org/types/process/biomaterial/4.0.0/well.json",
    "additionalProperties": false,
    "description": "This describes the well inside the plate or fluidics chip where biomaterial came from.",
    "title": "well",
    "properties": {
       "$schema" : {
            "pattern" : "http://schema.humancellatlas.org/types/process/biomaterial/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/well.json",
            "type": "string"
        },
        "col": {
            "description": "Well column in plate.",
            "type": "string"
        }, 
        "name": {
            "description": "A name for the well. Should be unique for the plate",
            "type": "string"
        }, 
        "plate": {
            "description": "An ID for the plate that the well is located on.",
            "type": "string"
        },
        "quality": {
            "description": "Note on how good cell looks if imaged in well before sequencing.",
            "enum": [
                "OK", 
                "control, 2-cell well", 
                "control, empty well", 
                "low quality cell"
            ]
        }, 
        "row": {
            "description": "Well row in plate.",
            "type": "string"
        }
    },
    "type": "object"
}
