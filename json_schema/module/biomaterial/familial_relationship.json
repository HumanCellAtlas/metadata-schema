{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://schema.humancellatlas.org/module/biomaterial/4.0.0/familial_relationship.json",
    "description": "Information relating to other organisms that this organism is related to.",
    "additionalProperties": false,
    "required": [
        "$schema"
    ],
    "title": "familial_relationship",
    "type": "object",
    "properties": {
        "$schema" : {
            "pattern" : "http://schema.humancellatlas.org/module/biomaterial/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/familial_relationship.json",
            "type": "string"
        },
        "schema_version": {
            "description": "Version number in major.minor.patch format.",
            "type": "string",
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
            "example": "4.6.1"
        }
    }
}
