{
    "description": "Contains information on single cell aspects of an assay.", 
    "title": "single_cell", 
    "required": [
        "cell_handling"
    ], 
    "additionalProperties": false, 
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "type": "object", 
    "properties": {
        "cell_barcode": {
            "description": "Information about cell identifier barcode.", 
            "$ref": "https://raw.githubusercontent.com/HumanCellAtlas/metadata-schema/4.4.0/json_schema/barcode.json"
        }, 
        "cell_handling": {
            "enum": [
                "10x_v2", 
                "FACS", 
                "Fluidigm C1", 
                "drop-seq", 
                "inDrop", 
                "mouth pipette", 
                "bulk", 
                "10x", 
                "10X"
            ], 
            "description": "How cells are separated. e.g. FACS, drop-seq, 10X_v2, Fluidigm C1."
        }
    }
}