{
    "$schema": "http://json-schema.org/draft-04/schema#",
     "id": "http://schema.humancellatlas.org/types/process/biomaterial/4.0.0/collection_process.json",
    "additionalProperties": false,
    "description": "Contains information on single cell aspects of an assay.",
    "required": [
    ],
    "title": "collection_process",
    "properties": {
        "$schema" : {
            "pattern" : "http://schema.humancellatlas.org/types/process/biomaterial/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/collection_process.json",
            "type": "string"
        },
        "process_core" : {
	        "description": "Core process-level information.",
            "$ref": "http://schema.humancellatlas.org/core/process/4.0.0/process_core.json"
        },
        "collection_method": {
            "description": "How the biomaterial was collected.",
            "type": "string",
            "enum": [
                "biopsy",
                "blood draw",
                "scraping",
                "urine collection",
                "organ extraction"
            ]
        }
    },
    "type": "object"
}
